{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/src/(.*)",
      "dest": "client/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}