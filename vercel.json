{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "echo 'No build step needed'",
  "publicDirectory": "client/static"
}
