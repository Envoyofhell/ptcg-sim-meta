{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/server.js"
        }
    ],
    "installCommand": "cd server && pnpm install",
    "buildCommand": "cd server && pnpm build"
}
