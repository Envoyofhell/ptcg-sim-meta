{
    "compatibility_date": "2024-03-24",
    "main": "dist/worker.js",
    "workers_dev": true,
    "routes": [
      {
        "pattern": "ptcg-sim-meta.pages.dev/api/*",
        "zone_name": "ptcg-sim-meta.pages.dev"
      },
      {
        "pattern": "ptcg-sim-meta.pages.dev/socket.io/*", 
        "zone_name": "ptcg-sim-meta.pages.dev"
      }
    ],
    "vars": {
      "ENVIRONMENT": "production",
      "LOG_LEVEL": "info",
      "SOCKET_ENABLED": "true"
    },
    "triggers": {
      "crons": [
        "0 0 * * *"
      ]
    },
    "compatibility_flags": [
      "nodejs_compat"
    ]
  }